// Instance population

si_population = header instance_block+ EOF
header = 'instances' EOL
instance_block = row+ block_end
row = '{ ' brace_val (SP brace_val)* ' }' EOL
brace_val = '[' val ']'
//comma_val =
val = r'[^\t\n\r\f\v\[\]]+'
//val = r'[^,\s\\]][^,\t\n\r\f\v]*'
block_end = "--" EOL
EOL = '\n'
SP = ' '